#include <string.h>
void pal(char c[],int k,int n){
     if(k==n||n-k==1){
         return 1;
     }
     if(c[k]==c[n]){
         return pal(c,k+1,n-1);
     }
     else{
         return 0;
     }
 }
int main()
{
    char s[100];
    int a=getchar()
    int i=0;
    while(a!='\0'){
        s[i]=a;
        i++;
        a=getchar();
    }
    if(pal(s,0,i-1)){
        printf("Yes");
    }
    else{
        printf("No");
    }
    return 0;
}6␀␀␀!by<Ã␀␀␀␀␀#include <stdio.h>
