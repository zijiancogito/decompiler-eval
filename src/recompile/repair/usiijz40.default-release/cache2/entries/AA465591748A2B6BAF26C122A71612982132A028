void reverse(char str1[],char str2[])
{
    int i,d,t=0,count1=0,count2=0,j;
    for(i=0;str1[i]!='\0';i++) count1++;
    for(i=0;str2[i]!='\0';i++) count2++;
    int p=count2;
    for(i=0;i<count1;i++)
    {  d=0;
        if(str1[i]==str2[t])
        {
          for(j=i;j<i+count2;j++)
          {
              if(str1[i]==str2[t])
              {
                d++;
                t++;
              }
          }
        }
        if(d==count2)
        {
            for(j=i;j<i+count2;j++)
            {
                str1[j]=str1[j+p];p--;
            }
        }
    }
    
}
int main() {
	char s1[100],s2[100];
	scanf("%s",str1);
	scanf("%s",str2);
	reverse(str1,str2);
	return 0;
}8␀␀␀ùöÆi␀␀␀␀␀␀#include<stdio.h>
