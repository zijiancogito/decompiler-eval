// Write any other auxillary functions here 

void replace_substring(char *s,int m,char *t,char *w,int r,int i)
{
    int j;
    for(j=i;j<i+m;j++){
        s[j]=w[j-i];
    }
    for(j=i+m;j<m+r;j++){
        s[j]=t[j];
    }
}

int main()
{
	int m,n,r,i,j,c=0;
	char*s,*t,*w,*p;
	scanf("%d",&m);
	s=(char*)malloc(sizeof(char)*(m+100));
	t=(char*)malloc(sizeof(char)*(m+100));
	for(i=0;i<m;i++){
	    scanf("%c",&s[i]);
	    t[i]=s[i];
	}
	scanf("%d",&n);
	w=(char*)malloc(sizeof(char)*n);
		for(i=0;i<m;i++){
	    scanf("%c",&s[i]);
	}
	scanf("%d",&r);
	p=(char*)malloc(sizeof(char)*r);
		for(i=0;i<m;i++){
	    scanf("%c",&s[i]);
	}
	for(i=0;i<m;i++){
	    if(s[i]==p[0]){
	        for(j=i;j<(i+n);j++){
	            if(s[i]==p[j-i]){
	                c++;
	            }
	        }
	        if(c==n){
	            replace_substring(s,t,w,i);
	            i=i-r;
	        }
	        c=0;
	    }
	}
	printf("%s",s);

	return 0;

